citEntry(entry = "Misc",
  title        = "BlandAltmanEdinburgh - a Bland Altman package for R",
  author       = personList(as.person("Deepankar Datta")),
  year         = "2015",
  textVersion  =
  paste("Deepankar Datta. (2015).",
        "BlandAltmanEdinburgh - a Bland Altman package for R.")
)
